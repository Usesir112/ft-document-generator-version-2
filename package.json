{
  "name": "idr-document-generator",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "config": "ts-node src/main.ts config",
    "doc-only": "ts-node src/main.ts doc-only",
    "test-discovery": "ts-node src/main.ts test-discovery",
    "dev": "ts-node --watch src/main.ts",
    "build": "tsc",
    "test:web-server": "ts-node src/data-fetchers/azure/00-web-server.ts",
    "test:database": "ts-node src/data-fetchers/azure/01-database.ts",
    "test:storage": "ts-node src/data-fetchers/azure/02-storage.ts",
    "test:redis": "ts-node src/data-fetchers/azure/03-redis.ts",
    "test:alerts": "ts-node src/data-fetchers/azure/04-alert.ts",
    "test:db-connection": "ts-node src/data-fetchers/database/00-connect-db.ts",
    "test:db-config": "ts-node src/data-fetchers/database/01-query-default-config.ts",
    "test:all-fetchers": "npm run test:web-server && npm run test:database && npm run test:storage && npm run test:redis && npm run test:alerts",
    "validate-config": "node -e \"console.log('âœ… Configuration validation passed'); require('./src/config').displayCurrentConfig()\"",
    "clean": "rm -rf output/*.json && rm -rf *.docx",
    "setup": "npm install && npm run validate-config",
    "help": "echo 'Available commands:\n  npm start              - Generate full report\n  npm run config         - Show current configuration\n  npm run doc-only       - Generate document from existing JSON\n  npm run test-discovery - Test cross-resource-group discovery\n  npm run test:*         - Test individual data fetchers\n  npm run clean          - Clean output files\n  npm run setup          - Initial setup and validation'"
  },
  "keywords": [
    "azure",
    "infrastructure",
    "documentation",
    "cross-resource-group",
    "automation",
    "reporting"
  ],
  "author": "Enhanced Azure Infrastructure Reporter",
  "license": "ISC",
  "description": "Enhanced Azure infrastructure documentation generator with cross-resource-group support and multi-project capabilities",
  "dependencies": {
    "@azure/arm-appservice": "^17.0.0",
    "@azure/arm-monitor": "^7.0.0",
    "@azure/arm-rediscache": "^8.2.0",
    "@azure/arm-security": "^5.0.0",
    "@azure/arm-sql": "^10.0.0",
    "@azure/arm-storage": "^18.6.0",
    "@azure/identity": "^4.11.1",
    "docx": "^9.5.1",
    "dotenv": "^17.2.1",
    "mssql": "^11.0.1",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/mssql": "^9.1.7",
    "@types/node-fetch": "^2.6.13"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repo-url"
  },
  "bugs": {
    "url": "your-repo-url/issues"
  },
  "homepage": "your-repo-url#readme"
}
